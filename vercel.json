{
    "version": 2,
    "builds": [
        {
            "src": "Furkan_itps_supportive_app.html",
            "use": "@vercel/static"
        },
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/Furkan_itps_supportive_app.html"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ]
}